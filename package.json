    "update-notifier": "2.5.0"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "eslint": "5.12.1",
    "eslint-config-es": "0.9.1",
    "jest": "24.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js"
  }
}
